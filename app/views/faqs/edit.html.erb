<div id="documentation_wrap">

<h1>Editing faq</h1>

<%= error_messages_for :faq %>

<% form_for(@faq) do |f| %>
  <p>
    <b>Title</b><br />
    <%= f.text_field :title %>
  </p>

  <p>
    <b>Body</b><br />
    <%= f.text_area :body %>
  </p>

  <p>
    <b>Published</b><br />
    <%= f.check_box :published %>
  </p>

  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @faq %> |
<%= link_to 'Back', faqs_path %>
</div>
